{
	"name": "FusedVR_Auth_API",
	"version": "0.3.0",
	"private": true,
	"scripts": {
		"dev:start": "cross-env NODE_ENV=dev node ./app/server.js",
		"dev:docs": "node docs/full-swagger-gen.js",
		"dev:build:start": "npm run prod:build && npm run dev:start",
		"prod:install": "npm install --only=prod",
		"prod:build": "tsc --project tsconfig.production.json",
		"prod:start": "cross-env NODE_ENV=prod node ./app/server.js",
		"prod:build:start": "npm run prod:build && npm run prod:start",
		"tests": "set NODE_ENV=dev && npm run prod:build && mocha --package package.json --recursive --exit -r app/env/.env.dev tests",
		"tests:controller": "mocha --package package.json --recursive --exit -r app/env/.env.dev tests/ControllerTests",
		"tests:routes": "mocha --package package.json --recursive --exit -r app/env/.env.dev tests/RoutesTests",
		"tests:utils": "mocha --package package.json --recursive --exit -r app/env/.env.dev tests/UtilitiesTests"
	},
	"dependencies": {
		"@walletconnect/client": "1.7.7",
		"body-parser": "latest",
		"cors": "latest",
		"cross-env": "latest",
		"dotenv": "^16.0.3",
		"ethereumjs-util": "latest",
		"express": "latest",
		"express-bearer-token": "latest",
		"express-fileupload": "latest",
		"express-validator": "latest",
		"express-winston": "latest",
		"jsonwebtoken": "latest",
		"moralis-v1": "latest",
		"nodemailer": "^6.8.0",
		"nodemailer-postmark-transport": "latest",
		"path": "latest",
		"swagger-jsdoc": "^6.2.5",
		"swagger-ui-express": "^4.6.0",
		"tslib": "latest",
		"web3": "^1.8.1",
		"winston": "^3.8.2"
	},
	"devDependencies": {
		"@types/ejs": "latest",
		"@types/express": "^4.17.14",
		"@types/express-fileupload": "^1.2.3",
		"@types/jsonwebtoken": "latest",
		"@types/nodemailer": "^6.4.6",
		"@types/swagger-jsdoc": "latest",
		"@types/swagger-ui-express": "latest",
		"@walletconnect/types": "1.7.5",
		"ethers": "latest",
		"http-server": "latest",
		"mocha": "latest",
		"moralis-admin-cli": "latest",
		"supertest": "latest",
		"typescript": "latest"
	}
}
